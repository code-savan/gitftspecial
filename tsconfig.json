{
    "compilerOptions": {
      "target": "ES2017",
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true,
      "jsx": "preserve",
      "moduleResolution": "node",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "incremental": true,
      "module": "esnext",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "plugins": [{ "name": "next" }],
      "baseUrl": ".",
      "paths": {
        "@/components/*": ["components/*"],
        "@/lib/*": ["lib/*"],
        "@/app/*": ["app/*"]
      }
    },
    "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "prisma/seed.ts"],
    "exclude": ["node_modules"]
  }
